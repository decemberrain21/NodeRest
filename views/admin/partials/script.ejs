		<script>
		
		function uploadphoto(iurl, data, cb)
			{
				
				var opts = {
					url: iurl,
					data: data,
					cache: false,
					//async: false,
					contentType: false,
					processData: false,
					type: 'POST',
					//timeout: 100000,
					success: function(res){
						cb(res);
					}
				};
				if(data.fake) {
					opts.xhr = function() { var xhr = $.ajaxSettings.xhr(); xhr.send = xhr.sendAsBinary; return xhr; }
					opts.contentType = "multipart/form-data; boundary="+data.boundary;
					opts.data = data.toString();
				}
				$.ajax(opts);
				

				
			}
			
			
			function csvupload(iurl, data, cb)
			{
				
				var opts = {
					url: iurl,
					data: data,
					cache: false,
					//async: false,
					contentType: false,
					processData: false,
					type: 'POST',
					timeout: 10000000,
					success: function(res){
						cb(res);
					}
				};
				if(data.fake) {
					opts.xhr = function() { var xhr = $.ajaxSettings.xhr(); xhr.send = xhr.sendAsBinary; return xhr; }
					opts.contentType = "multipart/form-data; boundary="+data.boundary;
					opts.data = data.toString();
				}
				$.ajax(opts);
				

				
			}
